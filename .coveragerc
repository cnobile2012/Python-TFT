[run]
parallel = True

omit =
    *tests*
    *mock*
    *examples*
    *fonts/font_convert.py

[report]
show_missing = True
skip_covered = True
precision = 3

# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

[html]
title = TFT LCD Display API
directory = docs/htmlcov
